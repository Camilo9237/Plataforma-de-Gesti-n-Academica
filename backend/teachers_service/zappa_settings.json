{
    "dev": {
        "app_function": "app.app",
        "aws_region": "us-east-1",
        "profile_name": "default",
        "project_name": "teachers-service",
        "runtime": "python3.11",
        "s3_bucket": "zappa-teachers-service-deployments",
        "manage_roles": true,
        "environment_variables": {
            "ENVIRONMENT": "development",
            "SERVICE_NAME": "teachers"
        },
        "cors": true,
        "api_key_required": false,
        "memory_size": 256,
        "timeout_seconds": 30,
        "keep_warm": false
    },
    "production": {
        "app_function": "app.app",
        "aws_region": "us-east-1",
        "profile_name": "default",
        "project_name": "teachers-service",
        "runtime": "python3.11",
        "s3_bucket": "zappa-teachers-service-deployments-prod",
        "manage_roles": true,
        "environment_variables": {
            "ENVIRONMENT": "production",
            "SERVICE_NAME": "teachers"
        },
        "cors": true,
        "api_key_required": false,
        "memory_size": 512,
        "timeout_seconds": 30,
        "keep_warm": true
    }
}